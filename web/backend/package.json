{
  "name": "shopify-app-template-node",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "debug": "node --inspect-brk index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --experimental-specifier-resolution=node index.ts",
    "build": "tsc --skipLibCheck",
    "prepare": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "serve": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/index.js",
    "start": "npm run prisma:migrate && npm run serve",
    "test": "vitest --reporter=verbose",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "pretty": "prettier --write ."
  },
  "type": "module",
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "@bull-board/express": "^5.1.2",
    "@paweljedrzejczyk/shopify-multistore-app-middleware": "^0.1.0",
    "@prisma/client": "^5.14.0",
    "@shopify/admin-graphql-api-utilities": "^2.0.1",
    "@shopify/shopify-api": "^7.0.0",
    "@shopify/shopify-app-express": "^2.1.0",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "mixpanel": "^0.18.0",
    "moment-timezone": "^0.5.43",
    "serve-static": "^1.14.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.9",
    "@types/nodemon": "^1.19.2",
    "@types/serve-static": "^1.15.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2"
  }
}
